#!/bin/bash

gg_sign_out() {
    xdotool mousemove 1746 104 # move to profile
    xdotool click 1
    sleep 2
    xdotool mousemove 971 1002 # move to sign out
    xdotool click 1
}

gg_sign_up() {
    xdotool mousemove 1709 108 # move to sign up
    xdotool click 1
    sleep 2
    xdotool key Tab
}

tm_del_and_copy() {
    xdotool mousemove 1043 229 # move to delete
    xdotool click 1
    sleep 9
    xdotool mousemove 1024 149 # move to copy
    xdotool click 1
}

gg_sign_out
